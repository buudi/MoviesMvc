@{
    ViewData["Title"] = "Just Experimenting with MVC Core";
}


<h1>@ViewData["Title"]</h1>

<h5>try add URL parameters, example: /home/experimental?name=Abdulah&NumTimes=50</h5>

<ol>
    @for (int i = 0; i < (int)ViewData["NumTimes"]!; i++)
    {
        <li>@ViewData["Message"] @Html.Raw(i+1) </li>
    }
</ol>